<div class="header">
  <h1>FAQs on Exporting</h1>
</div>
<div class="content">
  <form class="pure-form pure-form-stacked search" {{action "search" on="submit"}}>
    <fieldset>
      {{input class="pure-input-1" value=qField placeholder="Ask a question"}}
      <div class="pure-g">
        <div class="pure-u-1 pure-u-md-11-24 countries">
          
        </div>
        <div class="pure-u-1 pure-u-md-11-24">
        {{input type="submit" value="Search" class="pure-u-1 pure-u-md-11-24 pure-button pure-button-primary"}}
        </div>
      </div>
    </fieldset>
  </form>

  {{outlet}}
</div>
